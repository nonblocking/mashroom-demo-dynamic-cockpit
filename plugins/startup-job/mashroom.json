        {
            "name": "Mashroom Dynamic Cockpit Demo Startup Job",
            "type": "background-job",
            "bootstrap": "./dist/mashroom-bootstrap.js",
            "requires": [
                "Mashroom Portal WebApp"
            ],
            "defaultConfig": {
                "cronSchedule": "*/1 * * * * *"
            }
        }
    ]
}
