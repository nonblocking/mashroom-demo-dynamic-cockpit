{
    "$schema": "./../../node_modules/@mashroom/mashroom-json-schemas/schemas/mashroom-plugins.json",
    "devModeBuildScript": "build",
    "plugins": [
        {
            "name": "Mashroom Dynamic Cockpit Demo Theme Extension",
            "type": "portal-page-enhancement",
            "bootstrap": "./dist/mashroom-bootstrap.js",
            "pageResources": {
                "css": [{
                    "path": "cockpit-style.css",
                    "rule": "onlyOnCockpitPage"
                }]
            },
            "defaultConfig": {
                "resourcesRoot": "./dist"
            }
        }
    ]
}
