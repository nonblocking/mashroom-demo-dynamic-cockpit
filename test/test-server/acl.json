{
    "$schema": "../../node_modules/@mashroom/mashroom-json-schemas/schemas/mashroom-security-acl.json",
    "/portal/**": {
        "*": {
            "allow": {
                "roles": ["Authenticated"]
            }
        }
    },
    "/backend/**": {
        "*": {
            "allow": {
                "ips": ["127.0.0.1", "::1"]
            }
        }
    }
}
